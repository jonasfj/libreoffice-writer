#include "mailmergechildwindow.hrc"
#include "dbui.hrc"
#include "helpid.h"
#include <cmdid.h>

#define WIDTH  100
#define HEIGHT 16
FloatingWindow DLG_MAILMERGECHILD
{
    OutputSize = TRUE ;
    SVLook = TRUE ;
    Size = MAP_APPFONT ( WIDTH , HEIGHT ) ;
    Moveable = TRUE ;
    Closeable = FALSE ;
    Sizeable = FALSE ;
    Zoomable = TRUE ;
    HelpID = HID_MAILMERGECHILD ;
    Hide = TRUE ;
    Text [ de ] = "Serienbrief-Assistent";
    Text [ en-US ] = "Mail Merge Wizard";
    ToolBox TB_BACK
    {
        MenuStrings = TRUE ;
        SVLook = TRUE ;
        ItemList =
        {
            ToolBoxItem
            {
                Identifier = 1;
                HelpID = HID_RETURN_TO_MAILMERGE ;
                Left = TRUE;
                Text [ de ] = "Zurück zum Serienbrief-Assistenten" ;
                Text [ en-US ] = "Return to Mail Merge Wizard" ;
            };
        };
    };
    #define IMGLIST_IDS\
        IdList = \
        { \
            FN_SHOW_ROOT; \
        }; \
        IdCount = 1

    ImageList ILIST_TBX
    {
        Prefix = "sc";
        MaskColor = IMAGE_MASK_COLOR;
        IMGLIST_IDS;
    };

    ImageList ILIST_TBX_HC
    {
        Prefix = "sch";
        MaskColor = IMAGE_MASK_COLOR ;
        IMGLIST_IDS;
    };
};
