    String  ST_LESS
    {
        Text [ de ] = "<< Weninger" ;
        Text[ en-US ] = "<< Less";
    };

    Control LB_STATUS
    {
        Pos = MAP_APPFONT ( 12 , 101 ) ;
        Size = MAP_APPFONT ( 226 , 80 ) ;
        Border = TRUE;
    };
    FixedLine  FL_SEPARATOR
    {
        Pos = MAP_APPFONT ( 0 , 187 ) ;
        Size = MAP_APPFONT ( 250 , 8 ) ;
    };
    PushButton PB_STOP
    {
        Pos = MAP_APPFONT ( 141 , 201 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        Text [ de ] = "Stopp" ;
        Text[ en-US ] = "~Stop";
    };
    PushButton PB_CLOSE
    {
        Pos = MAP_APPFONT ( 194 , 201 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        Text [ de ] = "Schließen" ;
        Text[ en-US ] = "C~lose";
    };
    String     ST_CONTINUE
    {
        Text [ de ] = "Fortsetzen" ;
        Text[ en-US ] = "~Continue";
    };
    #define IMGLIST_IDS\
        IdList = \
        { \
            FN_FORMULA_CANCEL ;\
            FN_FORMULA_APPLY     ;\
        }; \
        IdCount = 2

    ImageList ILIST
    {
        Prefix = "sc";
        MaskColor = IMAGE_MASK_COLOR;
        IMGLIST_IDS;
    };

    ImageList ILIST_HC
    {
        Prefix = "sch";
        MaskColor = IMAGE_MASK_COLOR ;
        IMGLIST_IDS;
    };
    String ST_TASK
    {
        Text [ de ] = "Aktion" ;
        Text[ en-US ] = "Task";
    };
    String ST_STATUS
    {
        Text [ de ] = "Status" ;
        Text[ en-US ] = "Status";
    };
    String ST_SENDINGTO
    {
        Text [ de ] = "Senden an: %1" ;
        Text[ en-US ] = "Sending to: %1";
    };
    String ST_COMPLETED
    {
        Text [ de ] = "Erfolgreich versendet" ;
        Text[ en-US ] = "Successfully sent";
    };
    String ST_FAILED   
    {
        Text [ de ] = "Fehlgeschlagen (ungültige Adresse)" ;
        Text[ en-US ] = "Sending failed (invalid address)";
    };
    String ST_TERMINATEQUERY
    {
        Text [ de ] = "In Ihrem %PRODUCTNAME Postausgang sind noch Nachrichten zu versenden.\nMöchten Sie das Programm trotzdem beenden?" ;
        Text[ en-US ] = "There are still e-mail messages in your %PRODUCTNAME Outbox.\nWould you like to exit anyway?";
    };
};

