Accelerator RID_SMACCEL
{
    ItemList =
    {
        AI_UNDO
        AI_REDO
        AcceleratorItem
        {
            Identifier = SID_SELECT ;
            Key = KeyCode
            {
                Code = KEY_A ;
                Modifier1 = TRUE ;
            };
        };
        AcceleratorItem
        {
            Identifier = SID_NEXTMARK ;
            Key = KeyCode
            {
                Code = KEY_F4 ;
            };
        };
        AcceleratorItem
        {
            Identifier = SID_PREVMARK ;
            Key = KeyCode
            {
                Code = KEY_F4 ;
                Shift = TRUE ;
            };
        };
        AcceleratorItem
        {
            Identifier = SID_NEXTERR ;
            Key = KeyCode
            {
                Code = KEY_F3 ;
            };
        };
        AcceleratorItem
        {
            Identifier = SID_PREVERR ;
            Key = KeyCode
            {
                Code = KEY_F3 ;
                Shift = TRUE ;
            };
        };
        AcceleratorItem
        {
            Identifier = SID_DRAW ;
            Key = KeyCode
            {
                Code = KEY_F9 ;
            };
        };
    };
};









































